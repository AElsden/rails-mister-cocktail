<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1>Add Cocktails to list</h1>
      <%= simple_form_for(cocktail) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
      <div class="form-actions">
        <%= f.submit %>
      </div>
      <% end %>
    </div>

  </div>
</div>

